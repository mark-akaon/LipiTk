# - Config file for the LIPITK package
# It defines the following variables
#  LIPITK_INCLUDE_DIRS   - include directories for LIPITK
#  LIPITK_LIBRARIES      - libraries to link against
#  LIPITK_LIBRARIES_DIRS - library directories for LIPITK

# Tell the user project where to find our headers and libraries
set( LIPITK_INCLUDE_DIRS   "@LIPITK_INCLUDE_DIRS@" )
set( LIPITK_LIBRARIES      "@LIPITK_LIBRARIES@" )
set( LIPITK_LIBRARIES_DIRS "@LIPITK_LIBRARIES_DIRS@" )

